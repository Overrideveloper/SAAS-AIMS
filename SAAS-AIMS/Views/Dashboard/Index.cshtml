@section Scripts {
    <script async>
        $(document).ready(function () {
            $('div#stats').ready(function () {
                SessionCount();
                MemberCount();
                MaleCount();
                FemaleCount();
                MemberCurrent();
                FemaleCurrent();
                MaleCurrent();
                Dues();
            });
        });

        function SessionCount() {
            $.ajax({
                type: 'GET',
                url: '/Dashboard/SessionCount',
                datatype: 'json',
                traditional: true,
                contentType: "application/json",
                cache: false,
                success: function (session) {
                    $('div#session').text(session.session);
                    setTimeout(function () {
                        SessionCount();
                    }, 900000);
                },
                error: function (session) {
                    alert('error');
                }
            });
        }

        function MemberCount() {
            $.ajax({
                type: 'GET',
                url: '/Dashboard/Members',
                datatype: 'json',
                traditional: true,
                contentType: "application/json",
                cache: false,
                success: function (member) {
                    $('div#members').text(member.members);
                    setTimeout(function () {
                        MemberCount();
                    }, 900000);
                },
                error: function (member) {
                    alert('error');
                }
            });
        }

        function MaleCount() {
            $.ajax({
                type: 'GET',
