                    setTimeout(function () {
                        MemberCurrent();
                        console.log(currentmember);
                    }, 900000);
                },
                error: function (currentmember) {
                    alert('error');
                }
            });
        }

        function MaleCurrent() {
            $.ajax({
                type: 'GET',
