                    $('div#femalecurrent').text(currentfemale.currentfemale);
                    console.log(currentfemale);
                    setTimeout(function () {
                        FemaleCurrent();
                    }, 900000);
                },
                error: function (currentfemale) {
                    alert('error');
                }
            });
        }

        function Dues()
        {
            $.ajax({
                type: 'GET',
                url: '/Dashboard/TotalDues',
                datatype: 'json',
                traditional: true,
                contentType: "application/json",
                cache: false,
                success: function (dues) {
                    $('div#dues').text('N' + dues.dues);
                    console.log(dues);
                    setTimeout(function () {
                        Dues();
                    }, 900000);
                },
                error: function (dues) {
                    alert('error');
                }
            });
        }
    </script>
